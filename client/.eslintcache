setEmail(e.target.value);\n        break;\n      case 'password':\n        setPassword(e.target.value);\n        break;\n      case 'name':\n        setName(e.target.value);\n        break;\n      default:\n        break;\n    }\n  };\n  return (\n    <div className=\"login\">\n      <main className=\"bg-white max-w-lg mx-auto p-8 md:p-12 my-20 rounded-lg shadow-2xl\">\n        <section>\n          <h3 className=\"font-bold text-2xl\">Register</h3>\n        </section>\n\n        <section className=\"mt-10\">\n          <form className=\"flex flex-col\" onSubmit={onHandleSubmit}>\n            <div className=\"mb-6 pt-3 rounded\">\n              <label\n                className=\"block text-gray-700 text-sm font-bold mb-2 ml-3\"\n                htmlFor=\"Name\"\n              >\n                Name\n              </label>\n              <input\n                type=\"text\"\n                id=\"name\"\n                name=\"name\"\n                value={name}\n                onChange={onHandleChange}\n                className=\"rounded w-full text-gray-700 focus:outline-none border-b-4 border-gray-300 focus:border-green-600 transition duration-500 px-3 pb-3\"\n              />\n            </div>\n            <div className=\"mb-6 pt-3 rounded\">\n              <label\n                className=\"block text-gray-700 text-sm font-bold mb-2 ml-3\"\n                htmlFor=\"email\"\n              >\n                Email\n              </label>\n              <input\n                type=\"text\"\n                id=\"email\"\n                name=\"email\"\n                value={email}\n                onChange={onHandleChange}\n                className=\"rounded w-full text-gray-700 focus:outline-none border-b-4 border-gray-300 focus:border-green-600 transition duration-500 px-3 pb-3\"\n              />\n            </div>\n            <div className=\"mb-6 pt-3 rounded\">\n              <label\n                className=\"block text-gray-700 text-sm font-bold mb-2 ml-3\"\n                htmlFor=\"password\"\n              >\n                Password\n              </label>\n              <input\n                type=\"password\"\n                id=\"password\"\n                name=\"password\"\n                value={password}\n                onChange={onHandleChange}\n                className=\"rounded w-full text-gray-700 focus:outline-none border-b-4 border-gray-300 focus:border-green-600 transition duration-500 px-3 pb-3\"\n              />\n            </div>\n            <button\n              className=\"bg-green-600 my-3 hover:bg-green-500 text-white font-bold py-2 rounded shadow-lg hover:shadow-xl transition duration-200\"\n              type=\"submit\"\n              onSubmit={onHandleSubmit}\n            >\n              Register\n            </button>\n          </form>\n        </section>\n      </main>\n\n      <div className=\"max-w-lg mx-auto text-center mt-12 mb-6\">\n        <p className=\"text-green-600\">\n          Already have an account?{' '}\n          <Link to=\"/login\" className=\"font-bold hover:underline\">\n            Sign in\n          </Link>\n          .\n        </p>\n      </div>\n    </div>\n  );\n};\n\nexport default Register;\n","/Users/tannerbarcelos/OneDrive/Software Development/Projects/ParMe/client/src/components/Bullet.js",[],"/Users/tannerbarcelos/OneDrive/Software Development/Projects/ParMe/client/src/components/icons/Track.js",[],"/Users/tannerbarcelos/OneDrive/Software Development/Projects/ParMe/client/src/components/icons/Visit.js",[],"/Users/tannerbarcelos/OneDrive/Software Development/Projects/ParMe/client/src/components/icons/Insights.js",[],"/Users/tannerbarcelos/OneDrive/Software Development/Projects/ParMe/client/src/components/icons/Comment.js",[],"/Users/tannerbarcelos/OneDrive/Software Development/Projects/ParMe/client/src/components/icons/Collaborate.js",[],"/Users/tannerbarcelos/OneDrive/Software Development/Projects/ParMe/client/src/store.js",[],"/Users/